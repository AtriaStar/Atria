{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Atria": {
    "ShouldUseSwagger": true,
    "DatabaseString": "Host=localhost;Database=Atria;Username=user;Password=password",
    "DatabaseDetailedErrors": true,
    "AllowedOrigin": "https://localhost:7206",
    "AuthenticationTokenExpireDuration": "7.00:00:00",
    "ResetTokenExpireDuration": "00:15:00",

    "MinimumWseScore": 0.05,
    "MinimumUserScore": 0.5,

    "Weights": {
      "Name": 100,
      "ShortDescription": 50,
      "FullDescription": 30,
      "Changelog": 5
    },
    "ScorePower": 3,
    "ApiPrefix": "api"
  }
}
