@using Models

@if (CurrentUser is not null) {
    @Authenticated
}
else {
    @NotAuthenticated
}

@code {
    [CascadingParameter]
    private User? CurrentUser { get; set; }
    
    [Parameter]
    public RenderFragment? Authenticated { get; set; }
    
    [Parameter]
    public RenderFragment? NotAuthenticated { get; set; }
}